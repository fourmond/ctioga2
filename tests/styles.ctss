/* A ctioga style sheet */

gradient curve {
    line-style: Dots;
}

.bottom {
    line-style: 5,2;
}

#mcos {
    color: Orange;
    marker: Square;
}

.different .left {
    stroke-color: Purple;
}

.different .right, .different .top {
    decoration: no;
}

.different > curve {
    line-width: 4;
}